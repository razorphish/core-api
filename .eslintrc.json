{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-underscore-dangle": "off",
    "comma-dangle": "off",
    "array-bracket-spacing": ["error", "never"],
    "jsx-quotes": ["error", "prefer-single"],
    "class-methods-use-this": "off",
    "implicit-arrow-linebreak": "off",
    "operator-linebreak": "off",
    "indent": "off",
    "no-param-reassign": "off"
  }
}
